import math
print('pliktologise toys syntelestes tis exiswsis ax^2+bx+c=0')
a = float(input('arithmos a:'))
b = float(input('arithmos b:'))
c = float(input('arithmos c:'))
D = b**2-4*a*c
U = math.sqrt(D)
if D>0:
    r1 = (-b + U)/(2*a)
    r2 = (-b - U)/(2*a)
    print("oi exiswsh exei dyo pragmatikes rizes,ti r1=",r1,"kai th r2=",r2)
elif D==0:
    print("h exiswsh exei mia dipli pragmatiki riza th r1,2",(-b/(2*a)))
else:
    print("h ejiswsh den exei pragmatikes rizes")
    
